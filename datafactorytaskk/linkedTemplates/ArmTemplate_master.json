{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorytaskk"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"notebook_linked_service_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'notebook_linked_service'"
		},
		"source_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'source_data'"
		},
		"SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ssmssevers.database.windows.net"
		},
		"SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dataa"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Haripriya"
		},
		"notebook_linked_service_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0604-064721-b656qwbt"
		},
		"source_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://streastusdata.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorytaskk_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"notebook_linked_service_accessToken": {
						"value": "[parameters('notebook_linked_service_accessToken')]"
					},
					"source_data_accountKey": {
						"value": "[parameters('source_data_accountKey')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					},
					"notebook_linked_service_properties_typeProperties_existingClusterId": {
						"value": "[parameters('notebook_linked_service_properties_typeProperties_existingClusterId')]"
					},
					"source_data_properties_typeProperties_url": {
						"value": "[parameters('source_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}